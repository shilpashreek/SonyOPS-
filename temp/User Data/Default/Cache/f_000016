    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=90)";
    filter: alpha(opacity=90);
    /* width: 1px; */
}

    .ps-container:hover > .ps-scrollbar-y-rail:hover > .ps-scrollbar-y {
        background-color: #9c9c9c;
        width: 7px;
        opacity: 1;
    }


