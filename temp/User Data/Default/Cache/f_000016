@-webkit-keyframes opacity-hide {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
        /*display: none;*/
    }
}

@keyframes opacity-hide {
    from {
        /*display: block;*/
        opacity: 1;
    }

    to {
        opacity: 0;
        /*display: none;*/
    }
}

@-webkit-keyframes opacity-show {
    from {
        opacity: 0;
        /*display: none;*/
    }

    to {
        opacity: 1;
        /*display: block;*/
    }
}

@keyframes opacity-show {
    from {
        opacity: 0;
        /*display: none;*/
    }

    to {
        opacity: 1;
        /*display: block;*/
    }
}

